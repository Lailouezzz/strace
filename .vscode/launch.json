{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug C",
			"type": "codelldb",
			"request": "launch",
			"program": "${workspaceFolder}/strace",
			"args": ["-vc", "cat"],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}

